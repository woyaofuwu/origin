<?xml version="1.0" encoding="UTF-8"?>
<workbook>			
	<sheet desc="BBOSS成员用户状态查询">
		<header isshow="true" height="300">	            
		 <cell
				name="GROUP_ID"
				desc="集团客户编码"
				type="1"
				align="1"
				width="6000"
			/>
				<cell
				name="EC_SERIAL_NUMBER"
				desc="集团服务号码"
				type="1"
				align="1"
				width="6000"
			/>
		  <cell
				name="PRODUCT_OFFER_ID"
				desc="订购关系编码"
				type="1"
				align="1"
				width="6000"				
			/>
			<cell
				name="SERIAL_NUMBER"
				desc="手机号码"
				type="1"
				align="1"
				width="6000"
			/>
		
		 	<cell
				name="POSPECNAME"
				desc="商品名称"
				type="1"
				align="1"
				width="6000"
			/>


			<cell
				name="PRODUCTSPECNAME"
				desc="产品名称"
				type="1"
				align="1"
				width="6000"
				
			/>
