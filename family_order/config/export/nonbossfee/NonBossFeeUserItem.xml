<?xml version="1.0" encoding="GB2312"?>
<workbook>
<!--
		@name: column name
		@desc: column title
		@type: column type(1:string 2:numeric 3:datatime)
		@align: column align(1:left 2:center 3:right)
		@width: column width
		@format: column format(numeric format:0.00|#.##..., datatime format:yyyy-MM-dd...)
		@scale: scale(scale=100)
		@datasrc: data source(for example:SCHEDULE_SCHETYPE|#td_s_brand|TD_M_STAFF.STAFF_ID.STAFF_NAME(or TD_M_STAFF.STAFF_ID.DEPART_ID;TD_M_DEPART.DEPART_ID.DEPART_NAME))
		@dataid: translate column name(default value:name) 
	--> 
	<sheet desc="非出账客户参数">
		<header isshow="true" height="300">
		<cell name="DATA_ID"     desc="单位ID" type="1" align="1" width="3000" />
		<cell name="DATA_NAME"   desc="付款单位全称" type="1" align="1" width="6000" />
		<cell name="PARA_CODE2"  desc="纳税人识别号" type="1" align="1" width="3000" />
		<cell name="PARA_CODE3"  desc="地址、电话" type="1" align="1" width="8000" />
		<cell name="PARA_CODE4"  desc="开户行及账号" type="1" align="1" width="6000" />
		<cell name="EXC_PARAM_NAME"  desc="用户类型" type="1" align="1" width="3000" /> 
		<cell name="PARA_CODE1"  desc="纳税资质" type="1" align="1" width="3000" />
		<cell name="START_DATE"  desc="生效时间" type="1" align="1" width="3000" />
		<cell name="END_DATE"    desc="失效时间" type="1" align="1" width="3000" />
		<cell name="UPDATE_TIME" desc="更新时间" type="1" align="1" width="6000" />
		<cell name="REMARK"      desc="备注" type="1" align="1" width="3000" />
		</header>
	</sheet>
</workbook>