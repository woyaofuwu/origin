<?xml version="1.0" encoding="GB2312"?>
<workbook>
<!--
		@name: column name
		@desc: column title
		@type: column type(1:string 2:numeric 3:datatime)
		@align: column align(1:left 2:center 3:right)
		@width: column width
		@format: column format(numeric format:0.00|#.##..., datatime format:yyyy-MM-dd...)
		@scale: scale(scale=100)
		@datasrc: data source(for example:SCHEDULE_SCHETYPE|#td_s_brand|TD_M_STAFF.STAFF_ID.STAFF_NAME(or TD_M_STAFF.STAFF_ID.DEPART_ID;TD_M_DEPART.DEPART_ID.DEPART_NAME))
		@dataid: translate column name(default value:name)
	-->
	<sheet desc="有价卡赠送审批工单配置">
		<header isshow="true" height="300">
			<cell name="AREA_CODE" desc="分公司" type="1" align="1" width="3000"  datasrc="TD_M_AREA.AREA_CODE.AREA_NAME"/> 
			<cell name="STAFF_ID" desc="审批工单号" type="1" align="1" width="5000" />
			<cell name="TOTAL_AMOUNT" desc="审批总额" type="1" align="3"  width="5000" />
			<cell name="OPERA_STAFF_ID" desc="配置工号" type="1" align="1" width="3000"/>
			<cell name="COMMIT_TIME" desc="配置时间" type="1" align="1" width="5000" /> 
			<cell name="END_TIME" desc="截止时间" type="1" align="1" width="5000" />
			<cell name="REMARK" desc="备注" type="1" align="1" width="5000" />
		</header>
	</sheet>
</workbook>