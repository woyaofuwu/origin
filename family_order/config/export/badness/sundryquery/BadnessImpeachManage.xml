<?xml version="1.0" encoding="GB2312"?>
<workbook>
	<sheet desc="不良信息资料查询">
		<header isshow="true" height="300">
			<cell
				name="INFO_RECV_ID"
				desc="服务请求标识"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="BADNESS_INFO"
				desc="被举报号码/网站地址"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="RECV_CONTENT"
				desc="举报内容"
				type="1"
				align="1"
				width="8000"
			/>
			<cell
				name="REPORT_SERIAL_NUMBER"
				desc="举报手机号码"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="REPORT_CUST_NAME"
				desc="举报用户姓名"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="REPORT_CUST_PROVINCE"
				desc="举报用户归属省"
				type="1"
				align="1"
				width="3000"
				datasrc="BAD_PROVINCE_CODE"
			/>
			<cell
				name="RECV_PROVINCE"
				desc="举报受理省"
				type="1"
				align="1"
				width="3000"
				datasrc="BAD_PROVINCE_CODE"
			/>
			<cell
				name="REPORT_TIME"
				desc="用户举报时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm:ss"
			/>
			<cell
				name="BADNESS_INFO_PROVINCE"
				desc="被举报号码归属省"
				type="1"
				align="1"
				width="4000"
				datasrc="BAD_PROVINCE_CODE"
			/>
			<cell
				name="SERV_REQUEST_TYPE"
				desc="服务请求类别"
				type="1"
				align="1"
				width="6000"
				datasrc="BAD_INFO_SERV_REQUEST_TYPE"
			/>
			<cell
				name="REPORT_TYPE_CODE"
				desc="举报对象类型"
				type="1"
				align="1"
				width="3000"
				datasrc="REPORT_TYPE_CODE"
			/>

			<cell
				name="IMPORTANT_LEVEL"
				desc="重要程度"
				type="1"
				align="1"
				width="3000"
				datasrc="IMPORTANT_LEVEL"
			/>

			<cell
				name="STATE_NAME"
				desc="处理状态"
				type="1"
				align="1"
				width="4000"
				datasrc="BAD_INFO_STATE"
			/>
			<cell
				name="HASTEN_STATE"
				desc="催办状态"
				type="1"
				align="1"
				width="4000"
				datasrc="BAD_INFO_STATE"
			/>
			<cell
				name="REPEAT_REPORT_NAME"
				desc="是否重复举报"
				type="1"
				align="1"
				width="4000"
			/>
		</header>
	</sheet>
	
</workbook>