<?xml version="1.0" encoding="GB2312"?>
<workbook>
<!--
		@name: column name
		@desc: column title
		@type: column type(1:string 2:numeric 3:datatime)
		@align: column align(1:left 2:center 3:right)
		@width: column width
		@format: column format(numeric format:0.00|#.##..., datatime format:yyyy-MM-dd...)
		@scale: scale(scale=100)
		@datasrc: data source(for example:SCHEDULE_SCHETYPE|#td_s_brand|TD_M_STAFF.STAFF_ID.STAFF_NAME(or TD_M_STAFF.STAFF_ID.DEPART_ID;TD_M_DEPART.DEPART_ID.DEPART_NAME))
		@dataid: translate column name(default value:name) 
	--> 
	<sheet desc="积分礼品领取清单">
		<header isshow="true" height="300">
			<cell name="RULE_ID" desc="礼品ID" type="1" align="1" width="3000" />              
			<cell name="RULE_NAME" desc="礼品名称" type="1" align="1" width="8000" />          
			<cell name="SUPPLIER" desc="供应商" type="1" align="1" width="5000" />             
			<cell name="DEPART_AREA_CODE" desc="领取分公司" type="1" align="1" width="3000" /> 
			<cell name="DEPART_NAME" desc="领取网点" type="1" align="1" width="8000" />        
			<cell name="SCORE" desc="礼品积分值" type="1" align="1" width="3000" />            
			<cell name="SERIAL_NUMBER" desc="用户手机号" type="1" align="1" width="3000" />    
			<cell name="SCORE_VALUE" desc="积分单价" type="1" align="1" width="3000" />        
			<cell name="NO_TAX_PRICE" desc="不含税结算单价" type="1" align="1" width="3000"/>  
			<cell name="GET_NUM" desc="领取数量" type="1" align="1" width="3000"/>             
			<cell name="NO_TAX_AMOUNT" desc="不含税结算金额" type="1" align="1" width="3000"/> 
			<cell name="TAX_RATE" desc="税率" type="1" align="1" width="3000"/>                
			<cell name="TAX" desc="结算税额" type="1" align="1" width="3000"/>                 
			<cell name="TAX_AMOUNT" desc="含税结算金额" type="1" align="1" width="3000"/>      
			<!--<cell name="ACCEPT_MONTH" desc="结算月份" type="1" align="1" width="3000"/>        
			-->
			<cell name="UT_TIME" desc="礼品领取时间" type="1" align="1" width="3000"/> 
			<cell name="ET_TIME" desc="积分兑换时间" type="1" align="1" width="3000"/> 
			
			<cell name="USER_CITY_CODE" desc="开卡归属" type="1" align="1" width="3000"/>      
			<cell name="UPDATE_STAFF_ID" desc="操作工号" type="1" align="1" width="3000"/> 
			<cell name="GOODS_STATE" desc="状态" type="1" align="1" width="3000"/> 
		</header>
	</sheet>
</workbook>