<?xml version="1.0" encoding="GB2312"?>
<workbook>
<!--
		@name: column name
		@desc: column title
		@type: column type(1:string 2:numeric 3:datatime)
		@align: column align(1:left 2:center 3:right)
		@width: column width
		@format: column format(numeric format:0.00|#.##..., datatime format:yyyy-MM-dd...)
		@scale: scale(scale=100)
		@datasrc: data source(for example:SCHEDULE_SCHETYPE|#td_s_brand|TD_M_STAFF.STAFF_ID.STAFF_NAME(or TD_M_STAFF.STAFF_ID.DEPART_ID;TD_M_DEPART.DEPART_ID.DEPART_NAME))
		@dataid: translate column name(default value:name) 
	--> 
	<sheet desc="批量网厅开户信息导入"> 
		<header isshow="true" height="300">
			<cell
				name="ORDER_NO"
				desc="订单编码"
				type="1"
				align="1"
				width="4000"
				nullable="no"
			/>
			<cell
				name="SERIAL_NUMBER"
				desc="开户号码"
				type="1"
				align="2"
				width="4000"
				nullable="no"
			/>
			<cell
				name="CUST_NAME"
				desc="客户名称"
				type="1"
				align="2"
				width="4000"
				nullable="no"
			/>
			<cell
				name="PSPT_ID"
				desc="客户身份证号码"
				type="1"
				align="2"
				width="4000"
				nullable="no"
			/>
			<cell
				name="PSPT_ADDR"
				desc="证件地址"
				type="1"
				align="2"
				width="4000"
				nullable="no"
			/>
			<cell
				name="POST_ADDR"
				desc="邮寄地址"
				type="1"
				align="2"
				width="4000"
				nullable="no"
			/>
			<cell
				name="POST_PHONE"
				desc="联系电话"
				type="1"
				align="2"
				width="4000"
				nullable="no"
			/>
			<cell
				name="PICNAMERPATH"
				desc="免冠头像文件名"
				type="1"
				align="2"
				width="4000"
				nullable="yes"
			/>
		</header>
	</sheet>
</workbook>