<?xml version="1.0" encoding="GB2312"?>
<workbook>
	<sheet desc="不良信息资料查询">
		<header isshow="true" height="300">
			<cell
				name="REPORT_TYPE_NAME"
				desc="举报对象"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="REPORT_NAME"
				desc="举报分类"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="REPORT_SERIAL_NUMBER"
				desc="举报号码"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="BADNESS_INFO"
				desc="被举报号码归"
				type="1"
				align="1"
				width="4000"
			/>
			<cell
				name="RECV_CONTENT"
				desc="举报内容"
				type="1"
				align="1"
				width="8000"
			/>
			<cell
				name="REPORT_TIME"
				desc="用户举报时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm:ss"
			/>
			<cell
				name="BADNESS_INFO_PROVINCE"
				desc="被举报号码归属省"
				type="1"
				align="1"
				width="4000"
				datasrc="BAD_PROVINCE_CODE"
			/>
			<cell
				name="BADNESS_INFO_PROVINCE"
				desc="被举报号码归属地"
				type="1"
				align="1"
				width="4000"
				datasrc="BAD_PROVINCE_CODE"
			/>
			<cell
				name="BRAND_CODE"
				desc="被举报号码品牌"
				type="1"
				align="1"
				width="3000"
				datasrc="BADNESSINFO_BRAND"
			/>
			<cell
				name="CONTENT_TYPE_CODE"
				desc="内容分类"
				type="1"
				align="1"
				width="3000"
				datasrc="BAD_INFO_SERV_REQUEST_TYPE"
			/>
			<cell
				name="STATE_NAME"
				desc="处理状态"
				type="1"
				align="1"
				width="4000"
				datasrc="BAD_INFO_STATE"
			/>
			<cell
				name="DEAL_RAMARK"
				desc="处理意见"
				type="1"
				align="1"
				width="4000"
				datasrc="BAD_INFO_DEAL_RESULT"
			/>
			<cell
				name="RECV_IN_TYPE"
				desc="举报受理渠道"
				type="1"
				align="1"
				width="3000"
				datasrc="BADNESSINFO_ACCEPTCHANNEL"
			/>
			<cell
				name="INFO_RECV_ID"
				desc="服务请求标识"
				type="1"
				align="1"
				width="6000"
			/>
		</header>
	</sheet>
	
</workbook>