<?xml version="1.0" encoding="GB2312"?>
<workbook>
	<sheet desc="不良信息资料查询">
		<header isshow="true" height="300">
			<cell
				name="INFO_RECV_ID"
				desc="服务请求标识"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="BADNESS_INFO"
				desc="被举报号码/网站地址"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="RECV_CONTENT"
				desc="举报内容"
				type="1"
				align="1"
				width="8000"
			/>
			<cell
				name="DEAL_RAMARK"
				desc="处理意见"
				type="1"
				align="1"
				width="3000"
				datasrc="BAD_INFO_DEAL_RESULT"
			/>
			<cell
				name="REPORT_SERIAL_NUMBER"
				desc="举报手机号码"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="REPORT_CUST_NAME"
				desc="举报用户姓名"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="REPORT_CUST_LEVEL"
				desc="举报用户级别"
				type="1"
				align="1"
				width="3000"
				datasrc="IBOSS_LEVEL"
			/>
			<cell
				name="IMPORTANT_LEVEL"
				desc="重要程度"
				type="1"
				align="1"
				width="3000"
				datasrc="IMPORTANT_LEVEL"
			/>
			<cell
				name="REPORT_CUST_PROVINCE"
				desc="举报用户归属省"
				type="1"
				align="1"
				width="3000"
				datasrc="BAD_PROVINCE_CODE"
			/>
			<cell
				name="RECV_PROVINCE"
				desc="举报受理省"
				type="1"
				align="1"
				width="3000"
				datasrc="BAD_PROVINCE_CODE"
			/>
			<cell
				name="EPARCHY_CODE"
				desc="业务归属地市"
				type="1"
				align="1"
				width="4000"
			/>

			<cell
				name="TARGET_PROVINCE"
				desc="目标省代码"
				type="1"
				align="1"
				width="4000"
				datasrc="BAD_PROVINCE_CODE"
			/>
			<cell
				name="REPORT_TIME"
				desc="用户举报时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm:ss"
			/>
			<cell
				name="RECV_TIME"
				desc="用户提交时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm:ss"
			/>
			<cell
				name="DEAL_DATE"
				desc="回复时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm:ss"
			/>
			<cell
				name="SERV_REQUEST_TYPE"
				desc="服务请求类别"
				type="1"
				align="1"
				width="6000"
				datasrc="BAD_INFO_SERV_REQUEST_TYPE"
			/>
			<cell
				name="REPORT_TYPE_CODE"
				desc="举报对象类型"
				type="1"
				align="1"
				width="3000"
				datasrc="REPORT_TYPE_CODE"
			/>

		</header>
	</sheet>
	
</workbook>