<?xml version="1.0" encoding="GB2312"?>
<workbook>
	<sheet desc="邮寄信息列表">
		<header isshow="true" height="300">
			<cell
				name="ID"
				desc="服务号码"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="POST_NAME"
				desc="邮寄名称"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="POST_CONTENT_TEXT"
				desc="邮寄内容"
				type="1"
				align="1"
				width="3000"
			/>
			<cell
				name="POST_TYPESET_TEXT"
				desc="邮寄方式"
				type="1"
				align="1"
