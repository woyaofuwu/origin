<?xml version="1.0" encoding="GB2312"?>
<workbook>

	<sheet desc="集团产品欠费情况查询导出">
		<header isshow="true" height="300">
			<cell
				name="AREA_NAME"
				desc="集团归属市县"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="CUST_NAME"
				desc="集团名称"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="GROUP_ID"
				desc="集团编码"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="CUST_MANAGER_NAME"
				desc="集团客户经理"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="CUST_MANAGER_ID"
				desc="客户经理工号"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="ACCT_ID"
				desc="集团账户"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="PRODUCT_NAME"
				desc="集团产品名称"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="SERIAL_NUMBER"
				desc="产品编码"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="PRODUCT_OWE"
				desc="产品欠费(元)"
				type="2"
				align="1"
				width="6000"
				format="0.00"
				scale="100"
			/>
			<cell
				name="PRODUCT_STATE"
				desc="产品状态"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="PRODUCT_START_CYCLE"
				desc="欠费起始账期"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="PRODUCT_END_CYCLE"
				desc="最大欠费账期"
				type="1"
				align="1"
				width="6000"
			/>
		</header>
	</sheet>	
</workbook>