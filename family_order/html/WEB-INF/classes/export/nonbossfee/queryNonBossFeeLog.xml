<?xml version="1.0" encoding="GB2312"?>
<workbook>
<!--
		@name: column name
		@desc: column title
		@type: column type(1:string 2:numeric 3:datatime)
		@align: column align(1:left 2:center 3:right)
		@width: column width
		@format: column format(numeric format:0.00|#.##..., datatime format:yyyy-MM-dd...)
		@scale: scale(scale=100)
		@datasrc: data source(for example:SCHEDULE_SCHETYPE|#td_s_brand|TD_M_STAFF.STAFF_ID.STAFF_NAME(or TD_M_STAFF.STAFF_ID.DEPART_ID;TD_M_DEPART.DEPART_ID.DEPART_NAME))
		@dataid: translate column name(default value:name)
	-->
	<sheet desc="非出账业务查询列表">
		<header isshow="true" height="300">
			<!-- datasrc="TD_M_AREA.AREA_CODE.AREA_NAME" -->
			<cell name="FEE_NAME" desc="编码" type="1" align="1" width="3000" />
			<cell name="OPER_TYPE_NAME_DESC" desc="项目名称" type="1" align="1" width="6000" />
			<cell name="PAY_NAME_TYPE" desc="用户类型" type="1" align="1" width="3000"/>
			<cell name="OPER_FEE" desc="金额" type="2" align="3" width="3000" /> 
			<cell name="NOTAX_FEE" desc="不含税价款" type="2" align="3" width="3000" />
			<cell name="TAX_FEE" desc="税款" type="2" align="3" width="3000" />
			<cell name="TAX_TYPE" desc="应税类型" type="1" align="1" width="3000" />
			<cell name="TAX_RATE" desc="税率(%)" type="2" align="3" width="3000" />
			<cell name="INVOICE_TYPE" desc="发票项目" type="1" align="1" width="3000" />
			<cell name="INVOICE_PRINT_TYPE" desc="发票打印" type="1" align="1" width="3000" />
			<cell name="RSRV_STR4" desc="仅打印发票" type="1" align="1" width="3000" />
			<cell name="PAY_NAME_ALL" desc="用户全称" type="1" align="1" width="3000" />
			<cell name="RSRV_STR5" desc="补充说明" type="1" align="1" width="6000" />
			<cell name="IF_CENCER" desc="是否返销" type="1" align="1" width="1000" />
			<cell name="RSRV_STR8" desc="返销日期" type="1" align="1" width="3000" />
			<cell name="REMARK" desc="备注" type="1" align="1" width="6000" />
			<cell name="OPER_DATE" desc="初始录入日期" type="1" align="1" width="3000" />
			<cell name="RSRV_STR7" desc="打印日期" type="1" align="1" width="3000" />
			<cell name="info.RSRV_STR9)" desc="作废/冲红日期" type="1" align="1" width="3000" />
			<cell name="info.RSRV_STR10)" desc="冲红打印日期" type="1" align="1" width="3000" />
			<cell name="TICKET_TYPE" desc="发票类型" type="1" align="1" width="3000"  datasrc="INVOICE_TYPE"/>
			<cell name="TICKET_ID" desc="票号" type="1" align="1" width="3000" />
			<cell name="PAY_MODE_DESC" desc="付款方式" type="1" align="1" width="3000"  />
			<cell name="TRADE_STAFF_ID" desc="录入工号" type="1" align="1" width="3000" />
			<cell name="TRADE_DEPART_ID" desc="录入部门" type="1" align="1" width="3000" />
			<cell name="LOG_ID" desc="流水号" type="1" align="1" width="3000" />
		</header>
	</sheet>
</workbook>