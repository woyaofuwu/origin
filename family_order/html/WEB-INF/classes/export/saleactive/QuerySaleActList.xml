<?xml version="1.0" encoding="UTF-8"?>
<workbook>

	<sheet desc="营销活动查询信息">
		<header isshow="true" height="300">
			<cell
				name="PRODUCT_NAME"
				desc="活动名称"
				type="1"
				align="1"
				width="7000"
			/>
			<cell
				name="PACKAGE_ID"
				desc="营销包编码"
				type="1"
				align="1"
				width="4000"
			/>
			<cell
				name="PACKAGE_NAME"
				desc="营销包名称"
				type="1"
				align="1"
				width="6000"
			/>
			<cell
				name="START_DATE"
				desc="活动开始时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm"
			/>
			<cell
				name="END_DATE"
				desc="活动结束时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm"
			/>
			<cell
				name="RSRV_DATE1"
				desc="约定在网开始时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm"
			/>
			<cell
				name="RSRV_DATE2"
				desc="约定在网结束时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm"
			/>
			<cell
				name="ACCEPT_DATE"
				desc="受理时间"
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm"
			/>
			<cell
				name="CANCEL_DATE"
				desc="返销时间 "
				type="3"
				align="1"
				width="4000"
				format="yyyy-MM-dd HH:mm"
			/>
			<cell
				name="RSRV_STR23"
				desc="IMEI"
				type="1"
				align="1"
				width="4000"
			/>
			<cell
				name="ADVANCE_PAY"
				desc="预存话费(元)"
				type="1"
				align="1"
				width="4000"
			/>
			<cell
				name="OPER_FEE"
				desc="捆绑手机款(元)"
				type="1"
				align="1"
				width="4000"
			/>
			<cell
				name="FOREGIFT"
				desc="押金(元)"
				type="1"
				align="1"
				width="4000"
			/>
			<cell
				name="ACTIVE_TYPE"
				desc="活动类别"
				type="1"
				align="1"
				width="4000"
			/>
			<cell
				name="X_LIMIT_MONEY"
				desc="每月返还话费额(元)"
				type="1"
				align="1"
				width="6000"
			/>	
			<cell
				name="X_USETAG"
				desc="返还状态"
				type="1"
				align="1"
				width="3000"
				datasrc="PURCHASE_USER_TYPE"
			/>	
			<cell
				name="X_MONTHS"
				desc="需返还月数"
				type="1"
				align="1"
				width="4000"
			/>	
			<cell
				name="X_LEFT_MONTHS"
				desc="已返还月数"
				type="1"
				align="1"
				width="4000"
			/>
			<cell
				name="REMARK"
				desc="备注"
				type="1"
				align="1"
				width="8000"
			/>		
		</header>
	</sheet>
	
</workbook>