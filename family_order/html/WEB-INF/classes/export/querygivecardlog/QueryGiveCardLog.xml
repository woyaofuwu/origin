<?xml version="1.0" encoding="GB2312"?>
<workbook>
<!--
		@name: column name
		@desc: column title
		@type: column type(1:string 2:numeric 3:datatime)
		@align: column align(1:left 2:center 3:right)
		@width: column width
		@format: column format(numeric format:0.00|#.##..., datatime format:yyyy-MM-dd...)
		@scale: scale(scale=100)
		@datasrc: data source(for example:SCHEDULE_SCHETYPE|#td_s_brand|TD_M_STAFF.STAFF_ID.STAFF_NAME(or TD_M_STAFF.STAFF_ID.DEPART_ID;TD_M_DEPART.DEPART_ID.DEPART_NAME))
		@dataid: translate column name(default value:name)
	-->
	<sheet desc="有价卡赠送日志查询">
		<header isshow="true" height="300">
			<cell name="AREA_CODE" desc="分公司" type="1" align="1" width="3000" /> 
			<cell name="STAFF_ID" desc="审批工单号" type="1" align="1" width="3000" />
			<cell name="TOTAL_AMOUNT" desc="审批总金额(元)" type="1" align="3" width="6000"/>
			<cell name="DEVICE_PRICE" desc="赠送金额(元)" type="1" align="3" width="3000"/>
			<cell name="DEVICE_NUM" desc="赠送数量" type="1" align="3" width="3000" /> 
			<cell name="UPDATE_STAFF_ID" desc="操作工号" type="1" align="1" width="3000" />
			<cell name="UPDATE_TIME" desc="操作日期" type="1" align="1" width="6000" /> 
		</header>
	</sheet>
</workbook>	                      