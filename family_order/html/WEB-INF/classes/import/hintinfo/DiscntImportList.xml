<?xml version="1.0" encoding="GB2312"?>
<workbook>
	<!--导入表
		@name: column name
		@desc: column title
		@type: column type(1:string 2:numeric 3:datatime)
		@align: column align(1:left 2:center 3:right)
		@width: column width
		@format: column format(numeric format:0.00|#.##..., datatime format:yyyy-MM-dd...)
		@scale: scale(scale=100)
		@nullable: option[yes|no]
		@equsize: column equals size
		@minsize: column min size
		@maxsize: column mas size
		@datasrc: data source(for example:SCHEDULE_SCHETYPE|#td_s_brand|TD_M_STAFF.STAFF_ID.STAFF_NAME)
	-->
	<sheet name="HintInfo" desc="提示信息导入">
		<header isshow="true" height="300">
			<cell
				name="DISCNT_CODE"
				desc="优惠编码"
				type="1"
				align="2"
				width="3000"
				nullable="no"
			/>
			<cell
				name="DISCNT_EXPLAIN"
				desc="优惠描述"
				type="1"
				align="2"
				width="3000"
			/>
			<cell
				name="UPDATE_DEPART_ID"
				desc="更新部门"
				type="1"
				align="2"
				width="3000"
				nullable="no"
			/>
			<cell
				name="UPDATE_STAFF_ID"
				desc="更新员工"
				type="1"
				align="2"
				width="3000"
				nullable="no"
			/>
		</header>
	</sheet>
</workbook>