   select A.TRADE_ID,
          'A' ELEMENT_TYPE_CODE,
          A.DISCNT_GIFT_ID   ELEMENT_ID,
          B.DISCNT_GIFT_NAME ELEMENT_NAME,
          A.MODIFY_TAG,
          'TF_B_TRADE_SALE_DEPOSIT' TRADE_TAB_NAME,
          A.RSRV_TAG3 DETAIL_STATE_CODE
     from TF_B_TRADE_SALE_DEPOSIT A, TD_B_SALE_DEPOSIT B
    where A.TRADE_ID = :TRADE_ID
      and A.DISCNT_GIFT_ID = B.DISCNT_GIFT_ID
      and A.MODIFY_TAG IN ('0')