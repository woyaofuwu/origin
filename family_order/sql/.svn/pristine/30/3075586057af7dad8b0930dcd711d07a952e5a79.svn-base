SELECT t.GROUP_ID,t.BIZ_CODE,t.INST_ID,t.BIZ_NAME,t.SERV_CODE,t.SERIAL_NUMBER,t.START_DATE,
t.USER_ID,t.SERVICE_ID,t.BIZ_ATTR,T.ACCESS_NUMBER,T.SI_BASE_IN_CODE,
T.EC_BASE_IN_CODE,T.ADMIN_NUM,T.BIZ_TYPE_CODE,T.BIZ_IN_CODE,T.RSRV_NUM1,T.RSRV_NUM2,
T.RSRV_NUM3 CONFIRMFLAG,T.RSRV_NUM4,T.RSRV_NUM5,T.RSRV_STR1   
FROM TF_F_USER_GRP_PLATSVC t 
WHERE SYSDATE BETWEEN T.START_DATE AND T.END_DATE AND t.USER_ID =:USER_ID