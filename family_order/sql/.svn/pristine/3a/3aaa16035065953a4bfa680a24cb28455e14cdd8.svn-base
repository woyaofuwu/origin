SELECT T.REQ_ID,
       T.ACCEPT_MONTH,
       T.ACCEPT_DATE,
       T.TRADE_TYPE_CODE,
       T.IN_MODE_CODE,
       T.USER_ID,
       T.SERIAL_NUMBER,
       T.NET_TYPE_CODE,
       T.EPARCHY_CODE,
       T.CITY_CODE,
       T.TRADE_STAFF_ID,
