SELECT T.BUSIFORM_ID,
       T.BI_SN,
       T.BPM_TEMPLET_ID,
       T.TEMPLET_TYPE,
       T.BUSIFORM_OPER_TYPE,
       T.IN_MODE_CODE,
       T.BUSI_TYPE,
       T.BUSI_CODE,
       T.REMARK,
       T.EPARCHY_CODE,
       T.CITY_CODE,
       T.ACCEPT_DEPART_ID,
       T.UPDATE_DEPART_ID,
       T.UPDATE_STAFF_ID,
       T.ACCEPT_STAFF_ID,
       TO_CHAR(T.CREATE_DATE, 'YYYY-MM-DD HH24:MI:SS') CREATE_DATE,
       TO_CHAR(T.UPDATE_DATE, 'YYYY-MM-DD HH24:MI:SS') UPDATE_DATE
  FROM TF_BH_EWE T
 WHERE T.BUSIFORM_ID = :BUSIFORM_ID
