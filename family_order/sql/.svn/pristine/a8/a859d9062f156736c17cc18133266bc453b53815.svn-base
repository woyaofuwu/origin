SELECT U.PARTITION_ID,
       TO_CHAR(U.USER_ID) USER_ID,
       U.INST_ID,
       U.SHEET_TYPE,
       U.PRODUCT_NO,
       U.INSTANCE_NUMBER,
       U.CON_PRODUCT_NO,
       U.LINE_NUMBER,
       U.LINE_NAME,
       U.BIZ_SECURITY_LV,
       U.BAND_WIDTH,
       U.ROUTE_MODE,
       U.LINE_PRICE,
       U.LINE_STLBUG,
       U.LINE_STLCM,
       U.LINE_IPUSR,
       U.PROVINCE_A,
       U.CITY_A,
       U.AREA_A,
       U.COUNTY_A,
       U.VILLAGE_A,
       U.PORT_TYPE_A,
       U.PORT_INTERFACE_TYPE_A,
       U.PORT_CUSTOME_A,
       U.PORT_CONTACT_A,
       U.PORT_CONTACT_PHONE_A,
       U.PROVINCE_Z,
       U.CITY_Z,
       U.AREA_Z,
       U.COUNTY_Z,
       U.VILLAGE_Z,
       U.PORT_TYPE_Z,
       U.PORT_INTERFACE_TYPE_Z,
       U.PORT_CUSTOME_Z,
       U.PORT_CONTACT_Z,
       U.PORT_CONTACT_PHONE_Z,
       U.CUST_APPSERV_IPADDNUM,
       U.DO_MAINNAME,
       U.MAIN_DO_MAINADD,
       U.SUPPORT_MODE,
       U.AMOUNT,
       U.IS_CUSTOMER_PROVIDE_EQUIPMENT,
       U.CUSTOMER_DEVICE_MODE,
       U.CUSTOMER_DEVICE_TYPE,
       U.CUSTOMER_DEVICE_VENDOR,
       U.PHONE_PERMISSION,
       U.PHONE_LIST,
       TO_CHAR(U.START_DATE, 'yyyy-mm-dd hh24:mi:ss') START_DATE,
       TO_CHAR(U.END_DATE, 'yyyy-mm-dd hh24:mi:ss') END_DATE,
       TO_CHAR(U.UPDATE_TIME, 'yyyy-mm-dd hh24:mi:ss') UPDATE_TIME,
       U.STAFF_ID,
       U.DEPART_ID,
       U.REMARK,
       U.RSRV_NUM1,
       U.RSRV_NUM2,
       U.RSRV_NUM3,
       U.RSRV_STR1,
       U.RSRV_STR2,
       U.RSRV_STR3,
       U.RSRV_STR4,
       U.RSRV_STR5,
       TO_CHAR(U.RSRV_DATE1, 'yyyy-mm-dd hh24:mi:ss') RSRV_DATE1,
       TO_CHAR(U.RSRV_DATE2, 'yyyy-mm-dd hh24:mi:ss') RSRV_DATE2
       FROM TF_B_TRADE_USER_DATALINE T, TF_F_USER_DATALINE U WHERE T.TRADE_ID = TO_NUMBER(:TRADE_ID)
       AND T.ACCEPT_MONTH = TO_NUMBER(SUBSTR(:TRADE_ID, 5, 2))
	   AND T.MODIFY_TAG = '0'
       AND T.USER_ID = U.USER_ID
       AND U.PARTITION_ID = MOD(TO_NUMBER(T.USER_ID), 10000)
	   AND U.USER_ID = :USER_ID
       AND T.PRODUCT_NO = U.PRODUCT_NO
	   AND T.PRODUCT_NO = :PRODUCT_NO
       AND U.END_DATE > SYSDATE