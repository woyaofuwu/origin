SELECT 
  ACTIVITY_NUMBER,
  ACCEPT_MONTH,
  USER_ID,
  SERIAL_NUMBER,
  CITY_CODE,
  MONEY,
  ACCEPT_DATE,
  PRIZE_TYPE_CODE,
  EXEC_FLAG,
  EXEC_TIME,
  EXEC_INFO,
  RANDOM_NUM,
  RADIX,
  PRIZE_ODDS_1,
  PRIZE_ODDS_2,
  PRIZE_ODDS_3,
  PRIZE_ODDS_4,
  PRIZE_ODDS_5,
  PRIZE_ODDS_6,
  REMARK,
  REVC1,
  REVC2,
  REVC3,
  REVC4,
  REVC5
FROM 
  TF_B_FREEPHONEFEE
WHERE 1=1
  AND ACTIVITY_NUMBER=:ACTIVITY_NUMBER
  AND USER_ID=TO_NUMBER(:USER_ID)
  AND PARTITION_ID=MOD(TO_NUMBER(:USER_ID),10000)
  
