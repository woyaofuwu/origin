--IS_CACHE=N
SELECT A.DISCNT_CODE 套餐
FROM TF_F_USER_DISCNT A
WHERE A.USER_ID = TO_NUMBER(:USER_ID)
AND A.PARTITION_ID = MOD( TO_NUMBER(:USER_ID),10000)
 AND A.END_DATE BETWEEN SYSDATE AND
     TRUNC(LAST_DAY(SYSDATE)) + 1 - 1 / 86400
 AND A.END_DATE > SYSDATE
 AND A.END_DATE > A.START_DATE
 AND A.DISCNT_CODE IN
     (4801, 4802, 4803, 4804, 4805, 4809, 4813, 4471, 4453, 4454, 4455, 4456, 4457, 4472, 4479)
 AND ROWNUM < 2