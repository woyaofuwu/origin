UPDATE TD_M_RES_STOCKJOB T
   SET T.WARNNING_VALUE_U = CASE WHEN T.WARNNING_VALUE_U <= :WARNNING_VALUE_U THEN 0 ELSE T.WARNNING_VALUE_U - :WARNNING_VALUE_U END
 WHERE T.STOCK_POS = :STOCK_POS
   AND T.RES_TYPE_CODE = 'A'
   AND T.RSRV_TAG1 = 'A'
   AND T.CITY_CODE = :CITY_CODE
   AND EPARCHY_CODE = :EPARCHY_CODE
   AND T.WARNNING_VALUE_U >= 0