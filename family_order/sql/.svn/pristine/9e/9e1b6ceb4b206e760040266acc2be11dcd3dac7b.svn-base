INSERT INTO TL_FH_BLACKUSER
  (ACCEPT_MONTH,
   IN_TIME,
   SERIAL_NUMBER,
   USER_ID,
   USER_ATTR,
   USER_LEVEL,
   PROVINCE_CODE,
   EFFECT_TAG,
   BEGIN_TIME,
   END_TIME,
   OPERATE_FLAG,
   EXEC_TIME,
   PROCESS_TAG,
   UPDATE_TIME,
   UPDATE_STAFF_ID,
   UPDATE_DEPART_ID,
   RSRV_STR1,
   RSRV_STR2,
   RSRV_STR3,
   RSRV_TAG1,
   RSRV_TAG2,
   RSRV_TAG3,
   REMARK)
  SELECT ACCEPT_MONTH,
         IN_TIME,
         SERIAL_NUMBER,
         USER_ID,
         USER_ATTR,
         USER_LEVEL,
         PROVINCE_CODE,
         EFFECT_TAG,
         BEGIN_TIME,
         END_TIME,
         :OPERATE_FLAG,
         EXEC_TIME,
         :PROCESS_TAG,
         SYSDATE,
         :UPDATE_STAFF_ID,
         :UPDATE_DEPART_ID,
         RSRV_STR1,
         RSRV_STR2,
         RSRV_STR3,
         RSRV_TAG1,
         RSRV_TAG2,
         RSRV_TAG3,
         :REMARK
    FROM TL_F_BLACKUSER
   WHERE 1 = 1
     AND SERIAL_NUMBER = :SERIAL_NUMBER