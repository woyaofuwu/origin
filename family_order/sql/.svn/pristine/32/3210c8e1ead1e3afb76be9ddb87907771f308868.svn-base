SELECT TRADE_ID
  FROM TF_B_TRADE
 WHERE TRADE_TYPE_CODE = TO_NUMBER(:TRADE_TYPE_CODE)
   AND SERIAL_NUMBER = :SERIAL_NUMBER
   AND CANCEL_TAG = :CANCEL_TAG
UNION ALL
SELECT TRADE_ID
  FROM TF_BH_TRADE
 WHERE TRADE_TYPE_CODE = TO_NUMBER(:TRADE_TYPE_CODE)
   AND SERIAL_NUMBER = :SERIAL_NUMBER
   AND CANCEL_TAG = :CANCEL_TAG