--IS_CACHE=N
SELECT DECODE(COUNT(1),0,0,1) 是否受理
  FROM TF_F_USER_SVC A, TF_F_USER B
 WHERE A.USER_ID = TO_NUMBER(:USER_ID)
   AND A.PARTITION_ID = MOD(TO_NUMBER(:USER_ID), 10000)
   AND A.USER_ID = B.USER_ID
   AND A.PARTITION_ID = B.PARTITION_ID
   AND A.SERVICE_ID = '231'
   AND B.REMOVE_TAG = '0'
   AND A.START_DATE >= ADD_MONTHS(TO_DATE(:ACCEPT_MONTH, 'YYYYMM'), -2)
   AND A.START_DATE < TO_DATE(:ACCEPT_MONTH, 'YYYYMM')