SELECT T.SERIAL_NUMBER,T.ORDER_ID,T.TRADE_ID1,T.TRADE_ID2,T.COMMODITY_CODE,T.SERV_TYPE,T.SP_CODE,T.OPERATOR_CODE,T.MEMBER_TYPE,T.CHANNEL_SOURCE,T.RE_WAY,T.BOOK_TIME,T.CONFIRM_TIME,T.CREATE_TIME,T.CONFIRM_LOG,T.ELEMENT_ID,T.SOURCE_TAG,T.UPLOAD_TAG FROM
(SELECT T1.SERIAL_NUMBER,T1.ORDER_ID,T1.TRADE_ID1,T1.TRADE_ID2,T1.COMMODITY_CODE,T1.SERV_TYPE,T1.SP_CODE,T1.OPERATOR_CODE,T1.MEMBER_TYPE,
T1.CHANNEL_SOURCE,T1.RE_WAY,T1.BOOK_TIME,T1.CONFIRM_TIME,T1.CREATE_TIME,T1.CONFIRM_LOG,T1.ELEMENT_ID,T1.SOURCE_TAG,T1.UPLOAD_TAG,T1.RSRV_STR1,T1.RSRV_STR2,T1.RSRV_STR3,T1.RSRV_STR4,T1.RSRV_STR4,T1.RSRV_STR5
FROM TL_B_ACCEPTSECCONFIRM_LOG T1
UNION
SELECT T2.SERIAL_NUMBER,T2.ORDER_ID,T2.TRADE_ID1,T2.TRADE_ID2,T2.COMMODITY_CODE,T2.SERV_TYPE,T2.SP_CODE,T2.OPERATOR_CODE,T2.MEMBER_TYPE,
T2.CHANNEL_SOURCE,T2.RE_WAY,T2.BOOK_TIME,T2.CONFIRM_TIME,T2.CREATE_TIME,T2.CONFIRM_LOG,T2.ELEMENT_ID,T2.SOURCE_TAG,T2.UPLOAD_TAG,T2.RSRV_STR1,T2.RSRV_STR2,T2.RSRV_STR3,T2.RSRV_STR4,T2.RSRV_STR4,T2.RSRV_STR5
FROM TL_B_CANCELSECCONFIRM_LOG T2) T
WHERE T.SERIAL_NUMBER=:SERIAL_NUMBER
WHERE T.USER_ID=:USER_ID