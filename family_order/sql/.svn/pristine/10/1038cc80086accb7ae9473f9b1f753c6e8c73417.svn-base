  FROM ti_a_fee_notice
 WHERE exec_time BETWEEN TO_DATE(:START_DATE,'YYYY-MM-DD hh24:mi:ss') AND TO_DATE(:END_DATE,'YYYY-MM-DD hh24:mi:ss')
