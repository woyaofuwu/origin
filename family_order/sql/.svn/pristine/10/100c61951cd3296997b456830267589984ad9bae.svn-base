select A.* 
  from TF_B_TRADE_SALE_ACTIVE A,TF_BH_TRADE B
 where A.TRADE_ID=B.TRADE_ID 
   AND A.PRODUCT_ID=:PRODUCT_ID
	 AND A.PACKAGE_ID=:PACKAGE_ID
	 AND A.CAMPN_TYPE=:CAMPN_TYPE
	 AND B.SERIAL_NUMBER=:SERIAL_NUMBER
	 AND B.TRADE_TYPE_CODE='240'
	 AND B.SUBSCRIBE_STATE='9'
	 and sysdate < a.end_date
	 and a.start_date<a.end_date