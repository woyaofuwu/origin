--IS_CACHE=Y
SELECT a.SP_CODE SP_CODE,
       a.BIZ_CODE BIZ_CODE ,
       a.PRODUCT_NO PRODUCT_NO,
       a.BIZ_NAME BIZ_NAME,
       a.BIZ_TYPE,BIZ_TYPE_CODE,
       a.BIZ_ATTR BIZ_ATTR,
       a.BIZ_DESC BIZ_DESC,
       a.ACCESS_MODE ACCESS_MODE,
       a.ORDER_MODE ORDER_MODE,
       a.COUNT_SIDE1 COUNT_SIDE1,
       a.COUNT_SIDE2 COUNT_SIDE2,
       a.BILL_TYPE BILL_TYPE,
       a.PRICE PRICE,
       to_char(a.START_DATE,'yyyy-mm-dd hh24:mi:ss') START_DATE,
       to_char(a.END_DATE,'yyyy-mm-dd hh24:mi:ss')    END_DATE,
       a.COUNT_PCT COUNT_PCT,
       a.NUM_TIME NUM_TIME,
       a.NUM_DAY NUM_DAY,
       a.BIZ_STATE_CODE BIZ_STATE_CODE,
       a.USAGE_DESC USAGE_DESC,
       a.INTRO_URL INTRO_URL,
       a.BIZ_STATUS BIZ_STATUS,
       a.SERVICE_PHONE SERVICE_PHONE,
       a.CONTACT CONTACT,
       to_char(a.FIRST_DATE,'yyyy-mm-dd hh24:mi:ss') FIRST_DATE,
       a.FOREGIFT_TYPE FOREGIFT_TYPE,
       a.FOREGIFT FOREGIFT,
       a.OPR_SOURCE OPR_SOURCE,
       a.BIZ_PROCESS_TAG BIZ_PROCESS_TAG,
       a.SMS_PROCESS_TAG SMS_PROCESS_TAG,
       a.RECOGNIZE_CODE RECOGNIZE_CODE,
       a.NET_TAG NET_TAG,
       a.RSRV_STR1 RSRV_STR1,
       a.RSRV_STR2 RSRV_STR2,
       a.RSRV_STR3 RSRV_STR3,
       a.RSRV_STR4 RSRV_STR4,
       a.RSRV_STR5 RSRV_STR5,
       a.RSRV_STR6 RSRV_STR6,
       a.RSRV_STR7 RSRV_STR7,
       a.RSRV_STR8 RSRV_STR8,
       a.RSRV_STR9 RSRV_STR9,
       a.RSRV_STR10 RSRV_STR10,
       a.UPDATE_STAFF_ID UPDATE_STAFF_ID,
       a.UPDATE_DEPART_ID UPDATE_DEPART_ID,
       to_char(a.UPDATE_TIME,'yyyy-mm-dd hh24:mi:ss') UPDATE_TIME,
       a.FILE_NAME FILE_NAME
  FROM TD_M_OPERATION_SP a,TD_M_CORPORATION_SP b
 WHERE a.SP_CODE = b.SP_CODE
   AND a.BIZ_NAME like '%'||:BIZ_NAME||'%'
   AND b.sp_name like '%'||:SP_NAME||'%'
   and sysdate between a.start_date and a.end_date