SELECT *
  FROM TL_B_VALUECARD_DETAILED T
 WHERE 1 = 1
   AND T.TRADE_TYPE_CODE = '418'
   AND T.CUST_NUMBER IS NULL
   AND T.CITY_CODE = :CITY_CODE
   AND T.DEPART_CODE = :UPDATE_DEPART_ID
   AND T.AUDITORDERNUMBER = :STAFF_ID
   AND T.RSRV_STR5 >= :START_VALUECARD_SALE_TIME
   AND T.RSRV_STR5 <= :END_VALUECARD_SALE_TIME
   AND T.STATE_CODE = '0'
   AND T.CUST_NUMBER = :RSRV_STR7
 ORDER BY T.RSRV_STR5 DESC