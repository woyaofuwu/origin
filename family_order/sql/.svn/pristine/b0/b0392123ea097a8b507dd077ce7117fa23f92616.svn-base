SELECT P.SUBORDER_ID,
       P.GOODS_ID,
       P.PRODUCT_ID,
       P.PRODUCT_TYPE,
       P.SERVICEID_LIST,
       P.RSRV_STR1,
       P.RSRV_STR2,
       P.RSRV_STR3,
       P.RSRV_STR4,
       P.RSRV_STR5,
       S.SUBEXTORDER_ID,
       S.ORDER_ID,
       S.STATE,
       S.DEAL_STATE,
       S.DEAL_DESC,
       S.NUMBER_OPRTYPE,
       S.SERVICENO,
       S.SERVICENO_TYPE,
       S.NUMBER_BRAND,
       S.QUANTITY,
       S.NUMBER_PRICE,
       S.SIM_PRICE,
       S.LEGAL_NAME,
       S.CERTIFICATE_TYPE,
       S.CERTIFICATE_NO,
       S.ACTIVITY_CODE,
       S.ACTIVITY_TYPE,
       S.ACTIVITY_NAME,
       S.ACTIVITY_DESC,
       S.SUBTOTAL_FEE,
       S.ADJUST_FEE,
       S.ADJUST_REASON,
       S.ORDER_STATUS,
       S.GOODS_TITLE,
       S.AMOUNT,
       S.PRICE,
       S.GOODS_PROVINCE,
       S.GOODS_CITY,
       V.VAS_ID,
       V.BOSS_ID,
       V.VAS_NAME,
       V.VAS_TYPE,
       V.VAS_DESC,
       V.EFFECT_TIME,
       V.VAS_AREA,
       V.PRICE,
       V.PACKAGE_TONE,
       V.PACKAGE_FLOW,
       V.PACKAGE_SMS,
       V.PACKAGE_MMS,
       V.PACKAGE_VIDEOPHONE,
       V.PACKAGE_WIFI,
       V.PACKAGE_RANGE,
       V.EXCEED_TONE,
       V.EXCEED_FLOW,
       V.EXCEED_SMS,
       V.STATUS
  FROM TF_B_CTRM_GERLPRODUCT P, TF_B_CTRM_GERLSUBORDER S, TD_B_CTRM_VAS V
 WHERE P.SUBORDER_ID = S.SUBORDER_ID
   AND P.PRODUCT_ID = V.VAS_ID
   AND P.SUBORDER_ID = :SUBORDER_ID
