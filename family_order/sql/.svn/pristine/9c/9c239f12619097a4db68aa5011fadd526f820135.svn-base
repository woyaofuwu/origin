INSERT INTO TI_B_GRP_PLATSVC
      (SYNC_SEQUENCE, SYNC_DAY, MODIFY_TAG, TRADE_ID, PARTITION_ID, USER_ID,
       SERV_CODE, SERIAL_NUMBER, GROUP_ID, BIZ_TYPE_CODE, BIZ_CODE, BIZ_ATTR,
       BIZ_NAME, BIZ_STATE_CODE, BIZ_STATUS, BIZ_PRI, USAGE_DESC, INTRO_URL,
       BILLING_TYPE, BILLING_MODE, PRICE, PRE_CHARGE, CS_TEL, CS_URL, ACCESS_MODE,
       ACCESS_NUMBER, MAX_ITEM_PRE_DAY, MAX_ITEM_PRE_MON, IS_TEXT_ECGN,
       DEFAULT_ECGN_LANG, TEXT_ECGN_EN, TEXT_ECGN_ZH, FIRST_DATE, START_DATE,
       END_DATE, UPDATE_DEPART_ID, UPDATE_STAFF_ID, UPDATE_TIME, REMARK,
       RSRV_NUM1, RSRV_NUM2, RSRV_NUM3, RSRV_NUM4, RSRV_NUM5, RSRV_STR1,
       RSRV_STR2, RSRV_STR3, RSRV_STR4, RSRV_STR5, RSRV_DATE1, RSRV_DATE2,
       RSRV_DATE3, RSRV_TAG1, RSRV_TAG2, RSRV_TAG3, INST_ID, SERVICE_ID,
       BIZ_IN_CODE)
      SELECT :SYNC_SEQUENCE, :SYNC_DAY,
             DECODE(B.MODIFY_TAG, '0', '0', '1', '2', '2', '2', '2'), B.TRADE_ID,
             MOD(B.USER_ID, 10000), B.USER_ID, B.SERV_CODE, B.SERIAL_NUMBER,
             B.GROUP_ID, B.BIZ_TYPE_CODE, B.BIZ_CODE, B.BIZ_ATTR, B.BIZ_NAME,
             B.BIZ_STATE_CODE, B.BIZ_STATUS, B.BIZ_PRI, B.USAGE_DESC,
             B.INTRO_URL, B.BILLING_TYPE, B.BILLING_MODE, B.PRICE, B.PRE_CHARGE, B.CS_TEL,
             B.CS_URL, B.ACCESS_MODE, B.ACCESS_NUMBER, B.MAX_ITEM_PRE_DAY,
             B.MAX_ITEM_PRE_MON, B.IS_TEXT_ECGN, B.DEFAULT_ECGN_LANG,
             B.TEXT_ECGN_EN, B.TEXT_ECGN_ZH, B.FIRST_DATE, B.START_DATE,
             B.END_DATE, B.UPDATE_DEPART_ID, B.UPDATE_STAFF_ID, B.UPDATE_TIME,
             B.REMARK, B.RSRV_NUM1, B.RSRV_NUM2, B.RSRV_NUM3, B.RSRV_NUM4,
             B.RSRV_NUM5, B.RSRV_STR1, B.RSRV_STR2, B.RSRV_STR3, B.RSRV_STR4,
             B.RSRV_STR5, B.RSRV_DATE1, B.RSRV_DATE2, B.RSRV_DATE3, B.RSRV_TAG1,
             B.RSRV_TAG2, B.RSRV_TAG3, B.INST_ID, B.SERVICE_ID, B.BIZ_IN_CODE
        FROM TF_B_TRADE_GRP_PLATSVC B
       WHERE B.TRADE_ID = :TRADE_ID
         AND B.ACCEPT_MONTH = :ACCEPT_MONTH