select  CUST_NAME, CONTACT_SN, PSPT_TYPE_CODE, PSPT_ID, HOME_ADDR, SET_ADDR, PRE_PDTID, to_char(REG_DATE,'YYYY-MM-DD HH24:MI:SS') REG_DATE, UPDATE_TIME, UPDATE_STAFF_ID, UPDATE_DEPART_ID, REMARK, RSRV_NUM1, RSRV_NUM2, RSRV_NUM3, RSRV_NUM4, RSRV_NUM5, RSRV_STR1, RSRV_STR2, RSRV_STR3, RSRV_STR4, RSRV_STR5, RSRV_DATE1, RSRV_DATE2, RSRV_DATE3, RSRV_TAG1, RSRV_TAG2, RSRV_TAG3, INST_ID, audit_status,b.data_name data_name, c.DATA_NAME PSPT_TYPE_NAME
 from TF_F_WIDENET_BOOK a , 
( select DATA_ID,DATA_NAME from td_s_static where SUBSYS_CODE='CSM' and TYPE_ID = 'PRE_REG_STATUS')
b ,( SELECT DATA_ID,DATA_NAME FROM TD_S_STATIC WHERE TYPE_ID = 'TD_S_PASSPORTTYPE') c where a.reg_date between to_date(:PARA_CODE2,'YYYY-MM-DD') and to_date(:PARA_CODE3,'YYYY-MM-DD') + 1 and ( (trim(a.CONTACT_SN)=:SERIAL_NUMBER) or (:SERIAL_NUMBER=' ') ) and ( (trim(a.CUST_NAME)=:CUST_NAME) or (:CUST_NAME=' ') )  and ( (trim(a.AUDIT_STATUS)=:AUDIT_STATUS_PROP) or (:AUDIT_STATUS_PROP=' ') ) and    a.audit_status=b.data_id  and a.PSPT_TYPE_CODE = c.DATA_ID