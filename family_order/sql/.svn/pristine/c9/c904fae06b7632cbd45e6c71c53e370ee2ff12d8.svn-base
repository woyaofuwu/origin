SELECT TO_CHAR(T.USER_ID) USER_ID,T.SERIAL_NUMBER,T.USER_CLASS,T.BIRTHDAY,TO_CHAR(T.IN_DATE, 'yyyy-mm-dd hh24:mi:ss') IN_DATE,TO_CHAR(T.START_DATE, 'yyyy-mm-dd hh24:mi:ss') START_DATE,TO_CHAR(T.END_DATE, 'yyyy-mm-dd hh24:mi:ss') END_DATE FROM TF_F_USER_INFO_CLASS T WHERE 1=1 AND T.SERIAL_NUMBER = :SERIAL_NUMBER AND SYSDATE BETWEEN T.START_DATE AND T.END_DATE