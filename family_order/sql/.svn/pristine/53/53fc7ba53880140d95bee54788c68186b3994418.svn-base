INSERT INTO TF_F_CUST_FAMILYMEB
  SELECT HOME_CUST_ID,
         HOME_ID,
         MOD(TO_NUMBER(:MEMBER_CUST_ID), 10000),
         TO_NUMBER(:MEMBER_CUST_ID),
         NET_TYPE_CODE,
         SERIAL_NUMBER,
         CUST_NAME,
         NICK_NAME,
         BRAND_CODE,
         PRODUCT_ID,
         MEMBER_BELONG,
         MEMBER_ROLE,
         MEMBER_KIND,
         SCORE_VALUE,
         SHORT_CODE,
         MEMBER_INTEREST_CODE,
         JOIN_DATE,
         JOIN_STAFF_ID,
         JOIN_DEPART_ID,
         REMOVE_TAG,
         REMOVE_DATE,
         REMOVE_STAFF_ID,
         REMOVE_DEPART_ID,
         REMOVE_REASON,
         SYSDATE,
         :UPDATE_STAFF_ID,
         :UPDATE_DEPART_ID,
         RSRV_TAG2,
         RSRV_TAG3,
         RSRV_NUM1,
         RSRV_NUM2,
         RSRV_NUM3,
         RSRV_STR1,
         RSRV_STR2,
         RSRV_STR3,
         RSRV_STR4,
         RSRV_STR5,
         RSRV_STR6,
         RSRV_STR7,
         RSRV_STR8,
         RSRV_STR9,
         RSRV_STR10,
         RSRV_DATE1,
         RSRV_DATE2,
         RSRV_DATE3,
         RSRV_TAG1
    FROM TF_F_CUST_FAMILYMEB
   WHERE PARTITION_ID = MOD(TO_NUMBER(:RSRV_STR1), 10000)
     AND MEMBER_CUST_ID = TO_NUMBER(:RSRV_STR1)
     AND REMOVE_TAG = :REMOVE_TAG