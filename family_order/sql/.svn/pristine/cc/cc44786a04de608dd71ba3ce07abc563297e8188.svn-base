SELECT a.trade_id,a.order_id,a.bpm_id,a.trade_type_code,a.in_mode_code,a.priority,a.subscribe_state,a.next_deal_tag,a.product_id,a.brand_code,a.user_id,a.cust_id,a.acct_id,a.serial_number,a.cust_name,a.accept_date,a.accept_month,a.trade_staff_id,a.trade_depart_id,a.trade_city_code,a.trade_eparchy_code,a.term_ip,a.eparchy_code,a.city_code,a.olcom_tag,a.exec_time,a.finish_date,a.oper_fee,a.foregift,a.advance_pay,a.invoice_no,a.fee_state,a.fee_time,a.fee_staff_id,DECODE(a.cancel_tag,'0','正常','1','返销','未知['||cancel_tag||']') cancel_tag,a.cancel_date,a.cancel_staff_id,a.cancel_depart_id,a.cancel_city_code,a.cancel_eparchy_code,a.process_tag_set,a.rsrv_str1,a.rsrv_str2,a.rsrv_str3,a.rsrv_str4,a.rsrv_str5,a.rsrv_str6,a.rsrv_str7,a.rsrv_str8,a.rsrv_str9,a.rsrv_str10,a.remark
 FROM tf_b_trade a, tf_b_trade_score b
 WHERE a.trade_id=b.trade_id and a.serial_number=:SERIAL_NUMBER
 AND a.accept_date + 0 BETWEEN TO_DATE(:START_DATE, 'YYYY-MM-DD HH24:MI:SS') AND TO_DATE(:END_DATE, 'YYYY-MM-DD HH24:MI:SS')
 UNION ALL
