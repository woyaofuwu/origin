UPDATE TF_F_USER SET 
REMOVE_TAG = :REMOVE_TAG, DESTROY_TIME = to_date(:DESTROY_TIME,'yyyy-mm-dd hh24:mi:ss') , REMOVE_EPARCHY_CODE = :REMOVE_EPARCHY_CODE, REMOVE_CITY_CODE = :REMOVE_CITY_CODE, REMOVE_DEPART_ID = :REMOVE_DEPART_ID, REMOVE_REASON_CODE = :REMOVE_REASON_CODE, UPDATE_TIME = to_date(:UPDATE_TIME,'yyyy-mm-dd hh24:mi:ss') , UPDATE_STAFF_ID = :UPDATE_STAFF_ID, UPDATE_DEPART_ID = :UPDATE_DEPART_ID, REMARK = :REMARK
WHERE 
PARTITION_ID = MOD(to_number(:USER_ID),10000) and USER_ID = to_number(:USER_ID)