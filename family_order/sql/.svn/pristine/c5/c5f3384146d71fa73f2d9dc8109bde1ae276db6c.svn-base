       PRODUCT_ID,
       PACKAGE_ID,
       ELEMENT_ID,
       ELEMENT_TYPE_CODE,
       STATUS,
       TRADE_ID,
       ACCEPT_DATE,
       ACCEPT_RESULT,
       ERROR_RESULT,
       UPDATE_TIME,
       UPDATE_STAFF_ID,
       UPDATE_DEPART_ID,
       REMARK,
       RSRV_STR1,
       RSRV_STR2,
       RSRV_STR3,
       RSRV_STR4,
       RSRV_STR5,
       CTRM_PRODUCT_TYPE_CODE,
       CTRM_PRODUCT_SERVICEID
