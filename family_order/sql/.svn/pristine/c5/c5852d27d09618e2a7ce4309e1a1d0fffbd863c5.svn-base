SELECT PROMOTION_CODE,
       PROMOTION_NAME,
       BIZ_CATALOG,
       DEPOSIT_FEE,
       GRADE_LOW_LIMIT,
       PROMISE_DURATION,
       AVERAGE_RETURN,
       FIRST_OR_LAST,
       FORL_RETURN,
       RECHARGE_FEE,
       TERMINAL_ALLOWANCE,
       VALID_START_DATE,
       VALID_END_DATE,
       TIME_STAMP,
       TRANS_TYPE,
       CRM_FLAG,
       PRODUCT_ID,
       PACKAGE_ID,
       ACTION_CODE,
       DISCNT_CODE,
       ACTION_CODE1,
       DISCNT_CODE1,
       CONTRACT_ID,
       REMARK,
       RSRT_TAG1,
       RSRT_TAG2,
       RSRT_STR1,
       RSRT_STR2
  FROM TI_B_CUSTSALE_PROMOTION
 WHERE PROMOTION_CODE = :PROMOTION_CODE
   AND CRM_FLAG = :CRM_FLAG