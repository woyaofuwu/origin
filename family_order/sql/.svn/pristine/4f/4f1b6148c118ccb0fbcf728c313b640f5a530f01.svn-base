SELECT SERIAL_NUMBER,
       DECODE(IS_BLACK, '1', '黑名单', '白名单') IS_BLACK,
       IN_DATE IN_TIME
  FROM TF_F_REPORTERBALCK A
 WHERE 1 = 1
   AND (A.SERIAL_NUMBER = :REPORT_SERIAL_NUMBER OR
       :REPORT_SERIAL_NUMBER IS NULL)
   AND (A.IS_BLACK = :IS_BLACK OR :IS_BLACK IS NULL)
   AND (TRUNC(A.IN_DATE) >= TO_DATE(:REPORT_START_TIME, 'yyyy-mm-dd') OR
       :REPORT_START_TIME IS NULL)
   AND (TRUNC(A.IN_DATE) <= TO_DATE(:REPORT_END_TIME, 'yyyy-mm-dd') OR
       :REPORT_END_TIME IS NULL)
