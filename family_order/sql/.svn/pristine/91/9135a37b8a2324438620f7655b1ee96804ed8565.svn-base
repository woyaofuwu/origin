SELECT TO_CHAR(T.USER_ID) USER_ID,T.SERIAL_NUMBER,T.USER_CLASS,T.BIRTHDAY,TO_CHAR(T.IN_DATE, 'yyyy-mm-dd hh24:mi:ss') IN_DATE,TO_CHAR(T.START_DATE, 'yyyy-mm-dd hh24:mi:ss') START_DATE,TO_CHAR(T.END_DATE, 'yyyy-mm-dd hh24:mi:ss') END_DATE FROM UCR_CRM1.TF_F_USER_INFO_CLASS T WHERE 1=1 AND T.USER_ID =:USER_ID  AND  T.START_DATE < SYSDATE
 AND  T.END_DATE > SYSDATE