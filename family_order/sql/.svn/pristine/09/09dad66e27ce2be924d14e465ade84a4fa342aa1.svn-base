SELECT U.SERIAL_NUMBER, U.USER_ID
  FROM TF_F_USER U
 WHERE U.CUST_ID = :CUST_ID
   AND U.REMOVE_TAG = '0'
   AND EXISTS (SELECT 1
          FROM TF_F_USER_PRODUCT P
         WHERE P.USER_ID = U.USER_ID
           AND P.PARTITION_ID = MOD(U.USER_ID, 10000)
           AND P.MAIN_TAG = '1'
           AND P.BRAND_CODE IN ('G001', 'G002', 'G010'))