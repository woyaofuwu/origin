INSERT INTO TF_B_TERMINAL_ASSIGN_DETAIL
(LOG_ID,START_VALUE,END_VALUE,OPER_NUM,RES_TYPE_CODE,RES_KIND_CODE,
BRAND_CODE,MODEL_CODE,PROPERTY_ID,COLOR_CODE,DEAL_TAG)
SELECT :LOG_ID,A.FITTINGS_ID,A.FITTINGS_ID,'1','O','02',
A.FITTINGS_BRAND_CODE,A.FITTINGS_MODEL_CODE,A.PROPERTY_ID,A.FITTINGS_COLOR_CODE,'0'
FROM TF_R_FITTINGS_ARCH A
WHERE 1=1
AND (:EPARCHY_CODE IS NULL OR A.EPARCHY_CODE=:EPARCHY_CODE)
AND (:STOCK_ID IS NULL OR A.STOCK_ID=:STOCK_ID)
AND (:CURRENT_STATE IS NULL OR A.CURRENT_STATE=:CURRENT_STATE)
AND (:ASSIGN_TAG IS NULL OR A.ASSIGN_TAG=:ASSIGN_TAG)
AND A.PROPERTY_ID IN 
(
 SELECT RES_NO FROM TF_B_RES_BATCH_TMP B WHERE B.LOG_ID=:LOG_ID
)