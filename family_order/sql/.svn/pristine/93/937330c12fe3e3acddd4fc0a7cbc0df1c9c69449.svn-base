SELECT A.USER_ID, A.SERVICE_ID, B.ATTR_CODE, B.ATTR_VALUE
  FROM TF_F_USER_SVC A, TF_F_USER_ATTR B, TD_S_COMMPARA C
 WHERE A.PARTITION_ID = MOD(TO_NUMBER(:USER_ID), 10000)
   AND A.USER_ID = :USER_ID
   AND A.END_DATE > SYSDATE
   AND A.SERVICE_ID = :SERVICE_ID
   AND A.USER_ID = B.USER_ID
   AND A.PARTITION_ID = B.PARTITION_ID
   AND B.END_DATE > SYSDATE
   AND B.RELA_INST_ID = A.INST_ID
   AND B.ATTR_CODE = :ATTR_CODE
   AND C.SUBSYS_CODE = 'CSM'
   AND C.PARAM_ATTR = 4009
   AND C.PARA_CODE1 = :PARA_CODE1
   AND C.PARAM_CODE = TO_CHAR(A.SERVICE_ID)
   AND C.PARA_CODE2 = B.ATTR_VALUE
   AND C.END_DATE > SYSDATE