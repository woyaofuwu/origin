SELECT DISTINCT PARA_CODE1,PARA_CODE2
  FROM TD_S_COMMPARA A
 WHERE A.SUBSYS_CODE = 'CSM'
   AND A.PARAM_ATTR = '2105'
   AND (A.PARAM_CODE= :PARAM_CODE
   or A.PARAM_CODE= :PARAM_CODE1 
   or A.PARAM_CODE= :PARAM_CODE2 )
   AND A.PARA_CODE1 = :PARA_CODE1
   AND SYSDATE BETWEEN A.START_DATE AND A.END_DATE
UNION 
SELECT TO_CHAR(B.DISCNT_CODE) PARA_CODE1,C.ATTR_VALUE PARA_CODE2  FROM TF_F_USER_DISCNT B,TF_F_USER_ATTR C
WHERE B.USER_ID = C.USER_ID
AND B.DISCNT_CODE = :PARA_CODE1
AND B.INST_ID = C.INST_ID 
AND C.INST_TYPE='D' 
AND C.ATTR_CODE = '2105'
AND B.USER_ID = :USER_ID
AND SYSDATE BETWEEN C.START_DATE AND C.END_DATE
AND SYSDATE BETWEEN B.START_DATE AND B.END_DATE