SELECT T.BUSIFORM_NODE_ID,
       T.BUSIFORM_ID,
       T.STAFF_ID,
       T.TURN_STAFF_ID,
       T.ACCEPT_MONTH,
       T.EPARCHY_CODE,
       T.ACCEPT_DEPART_ID,
       T.UPDATE_DEPART_ID,
       T.UPDATE_STAFF_ID,
       T.ACCEPT_STAFF_ID,
       T.CREATE_DATE,
       T.UPDATE_DATE
  FROM TF_B_EWE_NODE_TUR T
 WHERE T.BUSIFORM_ID = :BUSIFORM_ID