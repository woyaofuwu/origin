INSERT INTO TI_B_USER_DEDUCT(
SYNC_SEQUENCE,
MODIFY_TAG,
REGISTER_ID,
EPARCHY_CODE,
CHANNEL_ID,
USER_ID,
ACCT_ID,
BANK_ACCOUNT_NO,
BANK_USRP_ID,
PAY_NAME,
PAY_ADDRESS,
PAY_POST_CODE,
DESTROY_TAG,
INMODE_CODE,
DEDUCT_TYPE_CODE,
DEDUCT_MONEY,
DEDUCT_STEP,
OPEN_TIME,
OPEN_CITY_CODE,
OPEN_DEPART_ID,
OPEN_STAFF_ID,
DESTROY_TIME,
DESTROY_CITY_CODE,
DESTROY_DEPART_ID,
DESTROY_STAFF_ID,
REMARK
) SELECT 
:SYNC_SEQUENCE,
:MODIFY_TAG,
REGISTER_ID,
EPARCHY_CODE,
CHANNEL_ID,
USER_ID,
ACCT_ID,
BANK_ACCOUNT_NO,
BANK_USRP_ID,
PAY_NAME,
PAY_ADDRESS,
PAY_POST_CODE,
DESTROY_TAG,
INMODE_CODE,
DEDUCT_TYPE_CODE,
DEDUCT_MONEY,
DEDUCT_STEP,
OPEN_TIME,
OPEN_CITY_CODE,
OPEN_DEPART_ID,
OPEN_STAFF_ID,
DESTROY_TIME,
DESTROY_CITY_CODE,
DESTROY_DEPART_ID,
DESTROY_STAFF_ID,
REMARK
FROM TF_F_USER_DEDUCT
WHERE DESTROY_TIME=(
SELECT MAX(DESTROY_TIME) FROM TF_F_USER_DEDUCT WHERE USER_ID=:USER_ID) AND USER_ID=:USER_ID AND DESTROY_TAG = '1'