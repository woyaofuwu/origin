SELECT DECODE(T.SUBSCRIBE_STATE,'0','00','1','01','2','01','3','01','4','01','5','01',
'6','01','7','01','8','01','9','02','A','04','B','01','C','01','D','04',
'E','01','F','01','G','01','H','01','I','03','M','04','P','01','L','01') ORDER_STATUS
 FROM (
(SELECT T1.SUBSCRIBE_STATE,T1.ACCEPT_MONTH,T1.TRADE_ID
FROM TF_B_TRADE T1
WHERE T1.BRAND_CODE='PWLW')
UNION ALL
(SELECT T2.SUBSCRIBE_STATE,T2.ACCEPT_MONTH,T2.TRADE_ID
FROM TF_BH_TRADE T2
WHERE T2.BRAND_CODE='PWLW')
) T
WHERE T.ACCEPT_MONTH = TO_NUMBER(SUBSTR(:TRADE_ID, 5, 2))
AND T.TRADE_ID = TO_NUMBER(:TRADE_ID)