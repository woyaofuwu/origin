SELECT A.RSRV_STR2,
       COUNT(a.trade_id) gift_count,
       A.TRADE_STAFF_ID,
       A.TRADE_DEPART_ID,
       A.TRADE_CITY_CODE
  FROM UCR_CRM1.TF_BH_TRADE_STAFF A
 WHERE  A.accept_month = to_number(substr(:START_DATE, 6, 2))
   AND A.TRADE_TYPE_CODE = '240'
   AND A.CANCEL_TAG = '0'
   AND A.RSRV_STR1 = '69900303'
   AND A.RSRV_STR2 IN ('60003761', '60003803', '60003762', '60003804',
        '60003763', '60003805', '60003764', '60003806')
   AND A.TRADE_STAFF_ID=:TRADE_STAFF_ID
   AND A.TRADE_DEPART_ID=:TRADE_DEPART_ID
   AND A.TRADE_CITY_CODE=:TRADE_CITY_CODE
   AND A.accept_date >= To_Date(:START_DATE, 'yyyy-mm-dd')
   AND A.accept_date <= To_Date(:END_DATE, 'yyyy-mm-dd')
   GROUP BY A.TRADE_CITY_CODE,A.TRADE_STAFF_ID,A.TRADE_DEPART_ID, A.RSRV_STR2