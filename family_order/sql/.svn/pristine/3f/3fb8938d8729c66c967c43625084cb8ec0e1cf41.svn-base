 WHERE A.TRADE_TYPE_CODE = 130
   AND A.RSRV_STR5 = :SERVICE_ID
   AND A.RSRV_STR7 = :OPER_CODE
   AND A.RSRV_STR9 = :SEND_FLAG
   AND A.USER_ID = :USER_ID
   AND A.ACCEPT_MONTH = TO_NUMBER( :ACCEPT_MONTH  )