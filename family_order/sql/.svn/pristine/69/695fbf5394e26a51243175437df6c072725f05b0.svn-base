SELECT 
to_char(t.INST_ID) INST_ID,
to_char(t.TRADE_ID) TRADE_ID, 
t.ACCEPT_MONTH, to_char(t.USER_ID) USER_ID, 
t.SERV_CODE, 
t.SERIAL_NUMBER, 
t.GROUP_ID, 
t.service_id,
t.BIZ_TYPE_CODE, 
t.BIZ_CODE, 
t.BIZ_ATTR, 
t.BIZ_NAME, 
t.BIZ_IN_CODE, 
t.BIZ_STATE_CODE, 
t.BIZ_STATUS, 
t.BIZ_PRI, 
t.OPER_STATE, 
t.AUTH_CODE, 
t.USAGE_DESC, 
t.INTRO_URL, 
t.BILLING_TYPE, 
t.BILLING_MODE, 
t.PRICE, 
t.PRE_CHARGE, 
t.CS_TEL, 
t.CS_URL, 
t.ACCESS_MODE, 
t.ACCESS_NUMBER, 
t.SI_BASE_IN_CODE, 
t.SI_BASE_IN_CODE_A, 
t.EC_BASE_IN_CODE, 
t.EC_BASE_IN_CODE_A, 
to_char(t.MAX_ITEM_PRE_DAY) MAX_ITEM_PRE_DAY, 
to_char(t.MAX_ITEM_PRE_MON) MAX_ITEM_PRE_MON, 
to_char(t.DELIVER_NUM) DELIVER_NUM, 
