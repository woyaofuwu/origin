to_char(t.OPR_EFF_TIME,'yyyy-mm-dd hh24:mi:ss') OPR_EFF_TIME, 
t.OPR_SEQ_ID, 
t.PLAT_SYNC_STATE, 
t.ADMIN_NUM, 
t.MAS_ID, 
to_char(t.START_DATE,'yyyy-mm-dd hh24:mi:ss') START_DATE, 
to_char(t.END_DATE,'yyyy-mm-dd hh24:mi:ss') END_DATE, 
