INSERT INTO TF_FH_USER_NP
  (USER_ID,
   NP_SERVICE_TYPE,
   SERIAL_NUMBER,
   PORT_OUT_NETID,
   PORT_IN_NETID,
   HOME_NETID,
   B_NP_CARD_TYPE,
   A_NP_CARD_TYPE,
   NP_TAG,
   APPLY_DATE,
   NP_DESTROY_TIME,
   PORT_IN_DATE,
   PORT_OUT_DATE,
   REMARK,
   RSRV_STR1,
   RSRV_STR2,
   RSRV_STR3,
   RSRV_STR4,
   RSRV_STR5)
  SELECT USER_ID,
         NP_SERVICE_TYPE,
         SERIAL_NUMBER,
         PORT_OUT_NETID,
         PORT_IN_NETID,
         HOME_NETID,
         B_NP_CARD_TYPE,
         A_NP_CARD_TYPE,
         NP_TAG,
         APPLY_DATE,
         NP_DESTROY_TIME,
         TO_DATE(TO_CHAR(SYSDATE, 'YYYY-MM-DD'), 'YYYY-MM-DD'),
         PORT_OUT_DATE,
         REMARK,
         RSRV_STR1,
         RSRV_STR2,
         RSRV_STR3,
         RSRV_STR4,
         RSRV_STR5
    FROM TF_F_USER_NP
   WHERE USER_ID = TO_NUMBER(:USER_ID)