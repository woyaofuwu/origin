SELECT TO_CHAR(TRADE_ID) TRADE_ID,
       CANCEL_TAG,
       PF_RESULTCODE RSRV_STR1,
       PFRTN_RESULTCODE RSRV_STR2,
       TO_CHAR(FINISH_DATE, 'YYYY-MM-DD HH24:MI:SS') FINISH_DATE,
       TO_CHAR(UPDATE_TIME, 'YYYY-MM-DD HH24:MI:SS') UPDATE_TIME
  FROM TF_B_TRADE_PBOSS_FINISH T
 WHERE T.TRADE_ID = TO_NUMBER(:TRADE_ID)
 AND T.ACCEPT_MONTH=TO_NUMBER(SUBSTR(:TRADE_ID,5,2))