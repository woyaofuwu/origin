SELECT A.*,f_sys_getcodename('staff_id',a.cust_manager_id,'','') CUST_MANAGER_NAME,TO_CHAR(A.ACCEPT_TIME,'YYYY-MM-DD HH24:MI:SS') AS ACCEPT_TIME_STR
FROM TF_B_GRP_BUSAPPLY A 
WHERE 1=1
AND A.OBJ_TYPE_CODE=:OBJ_TYPE_CODE
AND A.DEAL_STATE=:DEAL_STATE
AND A.CUST_MANAGER_ID=:CUST_MANAGER_ID
AND A.SERIAL_NUMBER=:SERIAL_NUMBER
AND A.ACCEPT_TIME >= TO_DATE(:START_TIME, 'YYYY-MM-DD HH24:MI:SS')
AND A.ACCEPT_TIME <= TO_DATE(:END_TIME, 'YYYY-MM-DD HH24:MI:SS')