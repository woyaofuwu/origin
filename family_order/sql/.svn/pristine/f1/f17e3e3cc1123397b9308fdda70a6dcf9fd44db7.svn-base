--IS_CACHE=Y
SELECT MIN(DEPART_ID) DEPART_ID,
       MIN('[' || DEPART_CODE || ']' || DEPART_NAME) DEPART_ID_NAME,
       MIN(DEPART_NAME) DEPART_NAME, DEPART_CODE,
       MIN(DEPART_KIND_CODE) DEPART_KIND_CODE, COUNT(*)
  FROM TD_M_DEPART
 WHERE 1 = 1
   AND PARENT_DEPART_ID = '36601'
   AND VALIDFLAG = '0'
   AND DEPART_ID <> '36601'
 GROUP BY DEPART_CODE
HAVING COUNT(*) >= 1
 ORDER BY DEPART_CODE