SELECT A.SERIAL_NUMBER,
       A.USER_ID,
       TO_DATE(A.OPEN_DATE,'YYYY-MM-DD HH24:MI:SS') AS OPEN_DATE ,
       A.DEVELOP_DATE,
       A.VALUEN2,
       B.AREA_CODE
  FROM TS_S_USER_BACK A, TD_M_DEPART B
 WHERE A.TAG = '0'
   AND A.DEVELOP_DEPART_ID = B.DEPART_ID
   AND B.VALIDFLAG = '0'
   AND B.END_DATE > SYSDATE
   AND A.SERIAL_NUMBER IN (:SERIAL_NUMBER)