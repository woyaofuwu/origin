SELECT TRADE_ID,ACCEPT_MONTH,USER_ID,ADDR_ID,ADDR_NAME,ADDR_DESC,MOFFICE_ID,MODIFY_TAG,TO_CHAR(START_DATE, 'YYYY-MM-DD HH24:MI:SS') START_DATE,TO_CHAR(END_DATE, 'YYYY-MM-DD HH24:MI:SS') END_DATE,TO_CHAR(UPDATE_TIME, 'YYYY-MM-DD HH24:MI:SS') UPDATE_TIME,UPDATE_STAFF_ID,UPDATE_DEPART_ID,REMARK,INST_ID 
  FROM TF_B_TRADE_ADDR WHERE TRADE_ID = :TRADE_ID AND ACCEPT_MONTH = TO_NUMBER(SUBSTR(:TRADE_ID, 5, 2))