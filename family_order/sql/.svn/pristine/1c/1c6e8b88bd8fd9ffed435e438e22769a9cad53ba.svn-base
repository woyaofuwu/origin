       CUMU_ACYC,
       CUMU_ID,
       OPER_CODE,
       USER_ID,
       MOBILE_NUM,
       OPEN_CHNL_ID,
       SCORE_TYPE,
       IN_TYPE,
       TO_CHAR(SEND_TIME,'YYYYMMDD HH24:MI:SS') SEND_TIME_SS,
       TO_CHAR(SEND_TIME,'YYYYMMDD') SEND_TIME,
       AREA_CODE,
       CAL_FLAG,
       KEEPMONTHID,
       SCORE_1,
       SCORE_2,
       PRE_CHAR1,
       PRE_CHAR2,
       PRE_NUM1,
       PRE_NUM2,
       RELATION_TRADE_ID
  FROM CHNL_CU_REGI_PARALLEL
