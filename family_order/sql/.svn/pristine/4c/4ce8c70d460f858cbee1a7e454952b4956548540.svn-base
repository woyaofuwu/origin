UPDATE TS_A_AUDITRESULT
   SET TRADE_NUMBER = :TRADE_NUMBER,
       DEVICE_NUM = :DEVICE_NUM,
       ERROR_NUM = :ERROR_NUM,
       AUDIT_TAG = :AUDIT_TAG,
       AUDIT_DATE = to_date(:AUDIT_DATE,'yyyy-mm-dd hh24:mi:ss'),
       AUDIT_STAFF_ID = :AUDIT_STAFF_ID,
       AUDITING_STAFF_ID = :AUDITING_STAFF_ID,
       RSRV_STR1 = :RSRV_STR1,
       RSRV_STR2 = :RSRV_STR2,
       RSRV_STR3 = :RSRV_STR3,
       RSRV_STR4 = :RSRV_STR4,
       RSRV_STR5 = :RSRV_STR5,
       RSRV_STR6 = :RSRV_STR6,
       RSRV_STR7 = :RSRV_STR7,
       RSRV_STR8 = :RSRV_STR8
 WHERE ACCEPT_DATE = to_date(:ACCEPT_DATE,'yyyy-mm-dd hh24:mi:ss')
   AND TRADE_DEPART_ID = :TRADE_DEPART_ID
   AND TRADE_STAFF_ID = :TRADE_STAFF_ID
   AND TRADE_TYPE_CODE = :TRADE_TYPE_CODE