SELECT COUNT(1) 记录数
    FROM CHNL_CU_REGI_PARALLEL A
   WHERE A.REGI_NUM = :REGI_NUM
    AND A.CAL_FLAG IN ('0','12','22')
    AND A.OPER_CODE IN ('07','21','22','23','29','37','38','39','40','43','44','92','94','104','105','106','145','237','238','239','240','292')
    AND CUMU_ACYC >= '201408'