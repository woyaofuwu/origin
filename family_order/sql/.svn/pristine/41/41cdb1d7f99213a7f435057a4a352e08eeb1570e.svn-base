select R.BUSI_CODE,
       R.BUSI_NAME,
       R.AREA_ID,
       R.IN_MODE_CODE,
       R.PRIORITY,
       R.BPM_TEMPLET_ID,
       R.BUSI_TYPE,
       R.BUSI_SPEC_ID,
       R.BEGIN_TIME,
       R.END_TIME,
       R.REMARK,
       R.RSRV_STR1,
       R.RSRV_STR2,
       R.RSRV_STR3,
       R.BUSI_OPER_TYPE
  from TD_B_EOP_BUSI_FLOW_RELE R
 WHERE R.BPM_TEMPLET_ID = :BPM_TEMPLET_ID
