UPDATE td_s_tag
   SET tag_number=TO_NUMBER(:TAG_NUMBER),remark=:REMARK,update_time=TO_DATE(:UPDATE_TIME, 'YYYY-MM-DD HH24:MI:SS'),update_staff_id=:UPDATE_STAFF_ID,update_depart_id=:UPDATE_DEPART_ID  
 WHERE eparchy_code=:EPARCHY_CODE
   AND tag_code=:TAG_CODE
   AND subsys_code=:SUBSYS_CODE
   AND use_tag=:USE_TAG