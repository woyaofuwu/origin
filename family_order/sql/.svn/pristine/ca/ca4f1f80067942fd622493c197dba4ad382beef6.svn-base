-----返销时新增一条标志位2的数据--
INSERT INTO TF_BH_TRADE
  (TRADE_ID,
   --SUBSCRIBE_ID,
   TRADE_TYPE_CODE,
   IN_MODE_CODE,
   PRIORITY,
   SUBSCRIBE_STATE,
   PRODUCT_ID,
   USER_ID,
   CUST_ID,
   SERIAL_NUMBER,
   ACCEPT_DATE,
   ACCEPT_MONTH,
   TRADE_STAFF_ID,
   TRADE_DEPART_ID,
   TRADE_CITY_CODE,
   TRADE_EPARCHY_CODE,
   EXEC_TIME,
   FINISH_DATE,
   FEE_STATE,
   CANCEL_TAG,
   PROCESS_TAG_SET,
   CANCEL_DATE,
   CANCEL_STAFF_ID,
   CANCEL_DEPART_ID,
   CANCEL_CITY_CODE,
   CANCEL_EPARCHY_CODE)
  SELECT TRADE_ID,
         --SUBSCRIBE_ID,
         TRADE_TYPE_CODE,
         IN_MODE_CODE,
         PRIORITY,
         SUBSCRIBE_STATE,
         PRODUCT_ID,
         USER_ID,
         CUST_ID,
         SERIAL_NUMBER,
         ACCEPT_DATE,
         ACCEPT_MONTH,
         TRADE_STAFF_ID,
         TRADE_DEPART_ID,
         TRADE_CITY_CODE,
         TRADE_EPARCHY_CODE,
         EXEC_TIME,
         FINISH_DATE,
         FEE_STATE,
         '2',
         PROCESS_TAG_SET,
         CANCEL_DATE,
         CANCEL_STAFF_ID,
         CANCEL_DEPART_ID,
         CANCEL_CITY_CODE,
         CANCEL_EPARCHY_CODE
    FROM TF_BH_TRADE
   WHERE TRADE_ID = :TRADE_ID
     AND ACCEPT_MONTH = SUBSTR(:TRADE_ID, 5, 2)