select operlog.* from TL_B_USPTRADE operlog where 1 = 1
 and operlog.BIZ_CODE = :BIZ_CODE
 and operlog.TRANS_CODE = :TRANS_CODE
 and operlog.INTF_CODE = :INTF_CODE
 and operlog.CHAN_ID = :CHAN_ID
 and operlog.TEST_FLAG = :TEST_FLAG
 and operlog.TRADE_ROUTE_TYPE = :TRADE_ROUTE_TYPE
 order by operlog.USP_ID DESC