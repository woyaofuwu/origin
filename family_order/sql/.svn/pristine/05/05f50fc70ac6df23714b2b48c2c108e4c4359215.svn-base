SELECT C4.DISCNT_CODE AS ELEMENT_ID, 'D' AS ELEMENT_TYPE_CODE
  FROM TF_B_TRADE_DISCNT C4
 WHERE C4.TRADE_ID = TO_NUMBER(:TRADE_ID)
   AND C4.ACCEPT_MONTH = TO_NUMBER(SUBSTR(:TRADE_ID, 5, 2))
UNION ALL
SELECT D4.SERVICE_ID AS ELEMENT_ID, 'S' AS ELEMENT_TYPE_CODE
  FROM TF_B_TRADE_SVC D4
 WHERE D4.TRADE_ID = TO_NUMBER(:TRADE_ID)
   AND D4.ACCEPT_MONTH = TO_NUMBER(SUBSTR(:TRADE_ID, 5, 2))