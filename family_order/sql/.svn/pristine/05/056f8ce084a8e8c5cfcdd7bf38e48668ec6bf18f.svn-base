UPDATE TI_CTRM_SHOP_INFO
   SET SHOP_TYPE     = :SHOP_TYPE,
       UPDATE_TIME      = to_date(:UPDATE_TIME, 'yyyy-MM-dd hh24:mi:ss'),
       SHOP_NAME = :SHOP_NAME,
       RSRV_STR1        = :RSRV_STR1,
	   RSRV_STR2	      = :RSRV_STR2,
       RSRV_STR3	      = :RSRV_STR3,
       RSRV_STR4	      = :RSRV_STR4,
       RSRV_STR5	      = :RSRV_STR5

 WHERE CHANNEL_CODE = :CHANNEL_CODE
 and SHOP_CODE = :SHOP_CODE