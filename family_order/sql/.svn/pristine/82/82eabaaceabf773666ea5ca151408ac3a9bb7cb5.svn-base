INSERT INTO TF_B_TRADE_ATTR(TRADE_ID,ACCEPT_MONTH,USER_ID,INST_TYPE,INST_ID,ATTR_CODE,ATTR_VALUE,START_DATE,END_DATE,MODIFY_TAG,UPDATE_TIME,UPDATE_STAFF_ID,UPDATE_DEPART_ID,REMARK)
SELECT TRADE_ID,ACCEPT_MONTH,USER_ID,'R'||RES_TYPE_CODE,INST_ID,:ATTR_CODE,:ATTR_VALUE,START_DATE,END_DATE,'0',UPDATE_TIME,UPDATE_STAFF_ID,UPDATE_DEPART_ID,REMARK
FROM  TF_B_TRADE_RES 
WHERE to_char(TRADE_ID) = :TRADE_ID
AND   to_char(ACCEPT_MONTH) = :ACCEPT_MONTH
AND   RES_TYPE_CODE = :RES_TYPE_CODE
AND   RES_CODE = :RES_CODE
AND   MODIFY_TAG = '0'